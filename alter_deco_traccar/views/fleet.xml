<data><button name="open_assignation_logs" position="before">
                <button name="open_vehicle_trip_history" type="object" class="oe_stat_button" title="Vehicle Trips" icon="fa-taxi">
                    <field name="trip_history_count" widget="statinfo" string="Traccar Trips"/>
                </button>
                <button name="open_vehicle_event_history" type="object" class="oe_stat_button" title="Vehicle Routes" icon="fa-car">
                    <field name="event_history_count" widget="statinfo" string="Traccar Routes"/>
                </button>
                <button name="open_device_event_log" type="object" class="oe_stat_button" title="Vehicle Events" icon="fa-history">
                    <span>Traccar Events</span>
                </button>
            </button>
            <xpath expr="//group" position="after">
                <group col="2" string="Traccar Details">
                    <group>
                        <field name="is_traccar" widget="boolean_toggle"/>
                        <label for="traccer_device_id" invisible="is_traccar == False"/>
                        <div invisible="is_traccar == False">
                            <field name="traccer_device_id" required="is_traccar == True"/>
                            <button class="oe_inline btn-success ml-2" name="validate_device" type="object" icon="fa-car" title="Vehicles" invisible="traccer_device_id in ['', False] or device_record_id not in [0, False]" help="Using this button you can fetch &amp; create device on traccar."/>
                        </div>
                        <label for="is_online"/>
                        <div>
                            <field name="is_online" readonly="1"/>
                            <button class="oe_inline btn-success ml-2" name="device_status" type="object" icon="fa-refresh" title="Device Status" help="click to fetch device status."/>
                        </div>
                        <field name="device_record_id" readonly="1"/>
                        <field name="device_name" readonly="1"/>
                        <field name="category" readonly="device_record_id &gt; 0" required="1"/>
                    </group>
                    <group>
                        <field name="device_long" readonly="1"/>
                        <field name="device_lat" readonly="1"/>
                        <field name="last_trip_update" readonly="1"/>
                        <field name="last_route_update" readonly="1"/>
                    </group>
                </group>
                <div class="w-100">
                    <div class="oe_inline">
                        <button name="open_device_summary" string=" View Device Summary" type="object" class="btn-success w-25 border border-white" icon="fa-list-alt" invisible="traccer_device_id in ['', False]"/>
                        <button name="fetch_trip_reports" string=" Fetch Trips" type="object" class="btn-success w-25 border border-white" icon="fa-car" title="Vehicles" invisible="traccer_device_id in ['', False]"/>
                        <button name="fetch_route_reports" string=" Fetch Routes" type="object" class="btn-success w-25 border border-white" icon="fa-car" title="Vehicles" invisible="traccer_device_id in ['', False]"/>
                        <button name="view_device_map" string=" View Current Location" type="object" class="btn-success w-25 border border-white" icon="fa-map-marker" invisible="traccer_device_id in ['', False]"/>
                    </div>
                </div>
            </xpath>
        </data>

        wizzard 

        <form string="Device Summary">
                <group string="Date Range">
                    <group col="2">
                        <field name="date_from" readonly="device_id != 0"/>
                    </group>
                    <group col="2">
                        <field name="date_to" readonly="device_id != 0"/>
                    </group>
                </group>
                <field name="message" invisible="device_id != 0"/>
                <group string="Summary Details" invisible="device_id == 0">
                    <group>
                        <field name="name"/>
                        <field name="device_id"/>
                        <field name="distance"/>
                        <field name="average_speed"/>
                        <field name="max_speed"/>
                    </group>
                    <group>
                        <field name="spent_fuel"/>
                        <field name="start_odometer"/>
                        <field name="end_odometer"/>
                        <field name="engine_hours"/>
                    </group>
                </group>
                <footer>
                    <button name="get_device_summary" string="Fetch Summary" type="object" class="btn btn-primary" invisible="device_id != 0"/>
                    <button special="cancel" class="oe_highlight btn-danger" icon="fa-archive" string="Close"/>
                </footer>
            </form>


            <form string="Tracking History">
                <field name="vehicle_id" invisible="1"/>
                <field name="driver_id" invisible="1"/>
                <group string="Date Range">
                    <group col="2">
                        <field name="date_from"/>
                    </group>
                    <group col="2">
                        <field name="date_to"/>
                    </group>
                </group>
                <footer>
                    <button name="get_event_reports" string="Fetch Route Reports" type="object" icon="fa-car" title="Vehicles" class="oe_highlight btn-success" invisible="context.get('Trip') == True"/>
                    <button name="get_trip_reports" string="Fetch Trip Reports" type="object" icon="fa-car" title="Vehicles" class="oe_highlight btn-success" invisible="context.get('Trip') == False"/>
                    <!-- <button name="show_event_reports" string="Show Event Report"
                        type="object" icon="fa-car" title="Vehicles" class="oe_highlight btn-success"
                        invisible= "not context.get('All') == True"/> -->
                    <button special="cancel" class="oe_highlight btn-danger" icon="fa-archive" string="Close"/>
                </footer>
            </form>



            ---config
            <form string="Connection Instance">
                <header>
                    <button name="test_connection" type="object" icon="fa-pencil-square-o" class="oe_highlight o_wow btn-dark" string="Test Connection"/>
                </header>
                <sheet>
                    <div class="oe_button_box" name="button_box">
                        <button name="toggle_active" type="object" class="oe_stat_button" icon="fa-check">
                            <field name="active" widget="boolean_toggle" options="{&quot;terminology&quot;: &quot;active&quot;}"/>
                            <span class="o_stat_text" invisible="active == False">Active</span>
                            <span class="o_stat_text" invisible="active == True">Inactive</span>
                        </button>
                    </div>
                    <widget name="web_ribbon" text="Connected" invisible="connection_status != True"/>
                    <widget name="web_ribbon" text="Not Connected" bg_color="bg-danger" invisible="connection_status != False or status == ''"/>
                    <field name="connection_status" invisible="True"/>
                    <span class="badge rounded-pill" invisible="connection_status != True" style="background-color:#1bab1b; color: #ffffff;">
                        success
                    </span>
                    <span class="badge rounded-pill" invisible="connection_status != False or status == ''" style="background-color:#fd3838; color: #ffffff;">
                        error
                    </span>
                    <h3>
                        <field name="status"/>
                    </h3>
                    <div class="oe_horizontal_separator oe_clear ">
                        <separator string="Traccar API Settings"/>
                    </div>
                    <group>
                        <label for="name"/>
                        <b class="o_form_required">
                            <field name="name" colspan="4" placeholder="Example: http://myecomm.com"/>
                        </b>
                    </group>
                    <notebook>
                        <page string="API Credentials">
                            <h2 style="margin-top: 10px !important;background-color: #e9ecef;padding-left: 8px;">
                                API Credentials
                            </h2>
                            <div id="api_reference">
                            </div>
                            <div class="row mt16 o_settings_container">
                                <div class="col-xs-12 col-md-6 o_setting_box">
                                    <div class="o_setting_right_pane">
                                        <label string="User Name" for="user"/>
                                        <div>
                                            <field name="user" placeholder="Example: apiuser"/>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-6 o_setting_box">
                                    <div class="o_setting_right_pane">
                                        <label string="Password" for="pwd"/>
                                        <div>
                                            <field name="pwd" placeholder="Example: apipassword"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </page>
                    </notebook>
                </sheet>
                <chatter/>
            </form>

            ____trip Details

            <form string="Vehicle Trip Details" create="0" edit="0" js_class="wk_map">
                <sheet>
                    <div class="oe_title">
                        <h1>
                            <field name="name" placeholder="e.g. TRIP0001"/>
                        </h1>
                        <label for="vehicle_id"/>
                        <h2>
                            <field name="vehicle_id" class="oe_inline"/>
                        </h2>
                        <label for="driver_id"/>
                        <h2>
                            <field name="driver_id" class="oe_inline"/>
                        </h2>
                        <group>
                            <group>
                                <field name="device_name"/>
                            </group>
                        </group>
                    </div>
                    <div class="row">
                        <div class="col w-50">
                            <group>
                            <group string="Start Latitude &amp; Longitude">
                                <field name="source_long" string="Longitude" readonly="1"/>
                                <field name="source_lat" string="Latitude" readonly="1"/>
                            </group>
                            <group string="End Latitude &amp; Longitude">
                                <field name="destination_long" string="Longitude" readonly="1"/>
                                <field name="destination_lat" string="Latitude" readonly="1"/>
                            </group>
                            
                            <group string="Speeds (In Knots)">
                                <field name="avr_speed" string="Average (kn)" readonly="1"/>
                                <field name="max_speed" string="Maximum (kn)" readonly="1"/>
                            </group>
                            <group string="Position">
                                <field name="start_position_id" string="Start" readonly="1"/>
                                <field name="end_position_id" string="End" readonly="1"/>
                            </group>
                            <group string="Odometer (In km)">
                                <field name="start_odo" string="Start" readonly="1"/>
                                <field name="end_odo" string="End" readonly="1"/>
                            </group>
                            <group string="Total Distance (In km)">
                                <label for="distance" string="Trip Distance"/>
                                <div class="o_row">
                                    <field name="distance"/> KM
                                </div>
                            </group>
                            <group string="Date">
                                <field name="start_date"/>
                                <field name="end_date"/>
                            </group>
                            <group string="Address">
                                <field name="start_address" string="Start Address" readonly="1"/>
                                <field name="end_address" string="End Address" readonly="1"/>
                            </group>
                            <!-- <group string="Traccar Event">
                                <field name="device_id"/>
                                <field name="device_unique_id"/>
                                <field name="device_name"/>
                            </group> -->
                            </group>
                        </div>
                        <div class="col w-50" id="traccarMap" route="true" style="height:500px;"/>
                    </div>
                </sheet>
            </form>